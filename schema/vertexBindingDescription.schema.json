{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "https://gitcdn.xyz/repo/jeffw387/VulkanSchema/master/schema/vertexBindingDescription.schema.json",
    "type": "object",
    "properties": {
        "binding": {
            "type": "integer"
        },
        "stride": {
            "type": "integer"
        },
        "inputRate": {
            "description": "typedef enum VkVertexInputRate {\nVK_VERTEX_INPUT_RATE_VERTEX = 0,\nVK_VERTEX_INPUT_RATE_INSTANCE = 1,\n} VkVertexInputRate;",
            "type": "integer",
            "enum": [
                0,
                1
            ]
        }
    },
    "required": [
        "binding",
        "stride",
        "inputRate"
    ]
}